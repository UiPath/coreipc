        "@types/node": "^18.11.6",
        "buffer": "^6.0.3",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "generate-package-json-webpack-plugin": "^2.6.0",
        "jest": "^29.2.2",
        "remove-files-webpack-plugin": "^1.5.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "ts-simple-nameof": "^1.3.1",
        "tsconfig-paths": "^4.1.0",
