    "main": "index.js",
    "scripts": {
        "clean": "npm run clean:dist & npm run clean:dist-packages",
        "clean:dist": "rimraf ./dist/**/*",
        "clean:dist-packages": "rimraf ./dist-packages/**/*.tgz",
        "build": "npm run clean & webpack --config webpack.dev.js",
        "build:node": "npm run clean & webpack --config webpack.dev.js --config-name node",
        "build:web": "npm run clean & webpack --config webpack.dev.js --config-name web",
        "build:web-js": "npm run clean & webpack --config webpack.dev.js --config-name web-js",
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --collect-coverage"
    },
    "devDependencies": {
        "@babel/core": "^7.19.6",
        "@babel/preset-env": "^7.19.4",
