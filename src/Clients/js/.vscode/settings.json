{
    "coverage-gutters.coverageFileNames": [
        "lcov.info",
        "cov.xml",
        "coverage.xml",
        "jacoco.xml"
    ],
    "coverage-gutters.showGutterCoverage": false,
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.partialHighlightLight": "rgba(220, 213, 143, 0.15)",
    "coverage-gutters.partialHighlightDark": "rgba(121, 86, 10, 0.15)",
    "coverage-gutters.highlightdark": "rgba(45, 121, 10, 0.15)",
    "coverage-gutters.highlightlight": "rgba(166, 220, 142, 0.15)",
    "coverage-gutters.noHighlightDark": "rgba(121, 31, 10, 0.15)",
    "coverage-gutters.noHighlightLight": "rgba(220, 143, 143, 0.15)",
    "testExplorer.mergeSuites": true,
    "mochaExplorer.autoload": true,
    "mochaExplorer.optsFile": "test/mocha.opts",
    "testExplorer.codeLens": true,
    "testExplorer.gutterDecoration": true,
    "testExplorer.sort": "byLabel",
    "typescript.tsdk": "node_modules\\typescript\\lib",
    "prettier.singleQuote": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": false,
        "source.fixAll": true,
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "node_modules": true
    }
}
