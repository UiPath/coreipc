{
    "coverage-gutters.coverageBaseDir": "**",
    "coverage-gutters.coverageFileNames": ["lcov.info", "cov.xml", "coverage.xml", "jacoco.xml"],
    "coverage-gutters.showGutterCoverage": false,
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.partialHighlightLight": "rgba(220, 213, 143, 0.15)",
    "coverage-gutters.partialHighlightDark": "rgba(121, 86, 10, 0.15)",
    "coverage-gutters.highlightdark": "rgba(45, 121, 10, 0.15)",
    "coverage-gutters.highlightlight": "rgba(166, 220, 142, 0.15)",
    "coverage-gutters.noHighlightDark": "rgba(121, 31, 10, 0.15)",
    "coverage-gutters.noHighlightLight": "rgba(220, 143, 143, 0.15)",
    "typescript.tsdk": "node_modules\\typescript\\lib",
    "prettier.singleQuote": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": false,
        "source.fixAll": true
    },

    // "mochaExplorer.files": ["test/node/**/*.ts", "test/std/**/*.ts"],
    // "mochaExplorer.require": "ts-node/register",
    // "mochaExplorer.logpanel": true,
    "karmaTestExplorer.testFiles": ["test/std/**/*.test.ts", "test/web/**/*.test.ts"],
    "jasmineExplorer.config": "jasmine.node.json",
    "jasmineExplorer.nodeArgv": [
        "--require",
        "ts-node/register",
        "--require",
        "tsconfig-paths/register"
    ],
    "jasmineExplorer.breakOnFirstLine": true
}
