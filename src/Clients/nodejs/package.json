{
    "name": "@uipath/coreipc",
    "version": "1.0.0",
    "main": "dist/index.js",
    "typings": "dist/index",
    "scripts": {
        "refreshVSToken": "vsts-npm-auth -config .npmrc",
        "build": "tsc",
        "clean:test": "rimraf reports",
        "clean:nyc": "rimraf .nyc_output",
        "pretest": "npm run clean:test & npm run lint",
        "test": "run-s test:*",
        "test:unit": "nyc mocha --opts ./test/mocha.opts",
        "test:report": "nyc report --reporter=html",
        "posttest": "nyc report --reporter=text-lcov > ./reports/coverage.lcov & nyc report --reporter=text",
        "lint": "tslint --project ./tsconfig.json",
        "coverage": "nyc mocha"
    },
    "dependencies": {
        "@types/node": "^12.6.8",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.0.0"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.spec.ts",
            "**/*.d.ts",
            "**/index.ts",
            "**/test/**/*",
            "**/testing/**/*"
        ],
        "reporter": [
            "html",
            "lcov",
            "json",
            "cobertura"
        ],
        "report-dir": "./reports/coverage",
        "cache": true,
        "all": true
    },
    "devDependencies": {
        "@types/chai": "4.1.7",
        "@types/chai-as-promised": "7.1.0",
        "@types/chai-spies": "1.0.0",
        "@types/fs-extra": "9.0.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^12.6.8",
        "@types/temp": "^0.8.34",
        "@types/uuid": "8.0.0",
        "chai": "4.2.0",
        "chai-as-promised": "7.1.1",
        "chai-spies": "1.0.0",
        "concurrently": "4.1.0",
        "fs-extra": "9.0.1",
        "istanbul-instrumenter-loader": "3.0.1",
        "madge": "^3.9.2",
        "mocha": "6.1.4",
        "mocha-junit-reporter": "1.23.1",
        "mocha-multi-reporters": "^1.1.7",
        "mocha-loader": "2.0.1",
        "mochawesome": "3.1.2",
        "npm-run-all": "^4.1.5",
        "nyc": "^14.1.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "2.6.3",
        "source-map-support": "0.5.12",
        "temp": "^0.9.1",
        "ts-node": "8.4.1",
        "ts-mockery": "^1.2.0",
        "tsconfig-paths": "^3.8.0",
        "tslint": "5.16.0",
        "tslint-consistent-codestyle": "1.15.1",
        "tslint-eslint-rules": "5.4.0",
        "typedoc": "^0.15.0",
        "typedoc-plugin-markdown": "1.2.0",
        "typescript": "3.9.7",
        "uuid": "8.0.0"
    }
}
