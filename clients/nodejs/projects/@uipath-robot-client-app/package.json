{
    "name": "@uipath/robot-client-app",
    "version": "0.0.24",
    "private": true,
    "description": "UiPath Robot Client Test App",
    "main": "dist/program.js",
    "bin": "dist/program.js",
    "typings": "dist/program",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "package": "pkg . --out-path ./bin",
        "debug": "node --nolazy --inspect-brk=9229 ./dist/program.js",
        "lint": "tslint --project \"./tsconfig.json\"",
        "start": "node ./dist/program.js",
        "start2": "node ./dist/program2.js",
        "restart": "tsc & node ./dist/program.js"
    },
    "author": "Eduard Dumitru",
    "license": "MIT",
    "files": [
        "dist/**/*"
    ],
    "pkg": {
        "assets": [
            "node_modules/blessed/usr/xterm",
            "node_modules/blessed/usr/xterm-256color"
        ]
    },
    "dependencies": {
        "@uipath/robot-client": "^0.0.24",
        "blessed": "0.1.81"
    },
    "devDependencies": {
        "@types/blessed": "0.1.10",
        "@types/jest": "^22.1.4",
        "@types/node": "^12.6.8",
        "pkg": "4.4.0",
        "typescript": "^3.6.4"
    },
    "gitHead": "edef4e238fde99232400d5270c6f54c3f054cb10"
}
